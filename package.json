{
  "name": "react-identicons",
  "version": "1.3.0",
  "private": true,
  "description": "Identicon component for React",
  "main": "dist/index.cjs.js",
  "module": "dist/index.esm.js",
  "source": "src/index.js",
  "scripts": {
